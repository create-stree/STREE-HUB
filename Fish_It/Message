MainTab:Section({
    Title = "Performance",
    Icon = "gamepad",
    TextXAlignment = "Left",
    TextSize = 17,
})

MainTab:Divider()

MainTab:Toggle({
    Title = "FPS Boost",
    Desc = "Optimizes performance for smooth gameplay",
    Icon = false,
    Type = false,
    Default = false,
    Callback = function(state)
        _G.FPSBoost = state
        local Lighting = game:GetService("Lighting")
        _G._FPSObjects = _G._FPSObjects or {}

        if state then
            if not _G.OldSettings then
                _G.OldSettings = {
                    GlobalShadows = Lighting.GlobalShadows,
                    FogEnd = Lighting.FogEnd,
                    Brightness = Lighting.Brightness,
                    Ambient = Lighting.Ambient,
                    OutdoorAmbient = Lighting.OutdoorAmbient,
                    ColorShift_Top = Lighting.ColorShift_Top,
                    ColorShift_Bottom = Lighting.ColorShift_Bottom,
                    ExposureCompensation = Lighting.ExposureCompensation
                }
            end

            Lighting.GlobalShadows = false
            Lighting.FogEnd = 1e10
            Lighting.Brightness = 0
            Lighting.ExposureCompensation = -1
            Lighting.Ambient = Color3.fromRGB(170,170,170)
            Lighting.OutdoorAmbient = Color3.fromRGB(170,170,170)
            Lighting.ColorShift_Top = Color3.new(0,0,0)
            Lighting.ColorShift_Bottom = Color3.new(0,0,0)

            for _,v in ipairs(Lighting:GetChildren()) do
                if v:IsA("BloomEffect")
                or v:IsA("SunRaysEffect")
                or v:IsA("ColorCorrectionEffect")
                or v:IsA("DepthOfFieldEffect")
                or v:IsA("BlurEffect")
                or v:IsA("Atmosphere") then
                    _G._FPSObjects[v] = {Enabled = v.Enabled}
                    v.Enabled = false
                end
            end

            for _,v in ipairs(workspace:GetDescendants()) do
                if not _G._FPSObjects[v] then
                    if v:IsA("BasePart") then
                        _G._FPSObjects[v] = {
                            Material = v.Material,
                            Color = v.Color,
                            CastShadow = v.CastShadow,
                            Reflectance = v.Reflectance
                        }
                        v.Material = Enum.Material.SmoothPlastic
                        v.Color = Color3.fromRGB(170,170,170)
                        v.CastShadow = false
                        v.Reflectance = 0
                    elseif v:IsA("Light") or v:IsA("Beam")
                    or v:IsA("ParticleEmitter") or v:IsA("Trail") then
                        _G._FPSObjects[v] = {Enabled = v.Enabled}
                        v.Enabled = false
                    end
                end
            end
        else
            if _G.OldSettings then
                Lighting.GlobalShadows = _G.OldSettings.GlobalShadows
                Lighting.FogEnd = _G.OldSettings.FogEnd
                Lighting.Brightness = _G.OldSettings.Brightness
                Lighting.Ambient = _G.OldSettings.Ambient
                Lighting.OutdoorAmbient = _G.OldSettings.OutdoorAmbient
                Lighting.ColorShift_Top = _G.OldSettings.ColorShift_Top
                Lighting.ColorShift_Bottom = _G.OldSettings.ColorShift_Bottom
                Lighting.ExposureCompensation = _G.OldSettings.ExposureCompensation
            end

            for obj,data in pairs(_G._FPSObjects) do
                if obj and obj.Parent then
                    if obj:IsA("BasePart") then
                        obj.Material = data.Material
                        obj.Color = data.Color
                        obj.CastShadow = data.CastShadow
                        obj.Reflectance = data.Reflectance
                    elseif obj.Enabled ~= nil then
                        obj.Enabled = data.Enabled
                    end
                end
            end

            table.clear(_G._FPSObjects)
        end
    end
})

