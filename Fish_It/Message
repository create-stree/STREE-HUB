    TextXAlignment = "Left",
    TextSize = 17
})

MainTab:Divider()

MainTab:Toggle({
    Title = "Auto Sell",
    Value = false,
    Callback = function(v)
        _G.AutoSell = v
        if v then
            if sellThread then task.cancel(sellThread) end
            sellThread = task.spawn(autosell)
        else
            _G.AutoSell = false
            if sellThread then task.cancel(sellThread) end
            sellThread = nil
        end
    end
})

